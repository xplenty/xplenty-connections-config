{
  "basic_auth":  {
    "auth_strategy": {
      "type": "com.xplenty.connections.strategies.auth.BasicAuthStrategy"
    }
  },
  "intercom":  {
    "auth_strategy": {
      "type": "com.xplenty.connections.strategies.auth.BearerAuthStrategy"
    }
  },
  "facebookadsinsights" : {
    "auth_strategy": {
      "type": "com.xplenty.connections.strategies.auth.BearerAuthStrategy"
    }
  },
  "google-analytics" : {
    "auth_strategy": {
      "type": "com.xplenty.connections.strategies.auth.BearerAuthStrategy"
    }
  },
  "salesforce" : {
    "auth_strategy": {
      "type": "com.xplenty.connections.strategies.auth.BearerAuthStrategy"
    }
  },
  "linkedin" : {
    "auth_strategy": {
      "type": "com.xplenty.connections.strategies.auth.BearerAuthStrategy"
    }
  },
  "shopify": {
    "auth_strategy": {
      "type": "com.xplenty.connections.strategies.auth.CustomAuthStrategy",
      "config": {
        "headers": {
          "X-Shopify-Access-Token" : "${access_token}"
        }
      }
    }
  },
  "google-speadsheets" : {
    "auth_strategy": {
      "type": "com.xplenty.connections.strategies.auth.BearerAuthStrategy"
    }
  }
}